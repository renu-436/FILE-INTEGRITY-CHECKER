import socket

def run(target_ip):
    print(f"[+] Scanning ports on {target_ip}")
    for port in range(1, 1025):
        try:
            sock = socket.socket()
            sock.settimeout(0.5)
            sock.connect((target_ip, port))
            print(f"[+] Port {port} is open")
            sock.close()
        except:
            pass
